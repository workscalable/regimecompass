{
  "name": "gamma-adaptive-system",
  "version": "1.0.0",
  "description": "Gamma Adaptive Trading System with comprehensive performance monitoring, caching, and security",
  "main": "index.js",
  "scripts": {
    "test": "jest --config tests/jest.config.js",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:integration": "jest --config tests/jest.config.js --testPathPattern=integration",
    "test:acceptance": "jest --config tests/jest.config.js --testPathPattern=acceptance",
    "test:sit": "jest --config tests/jest.config.js --testPathPattern=integration",
    "test:car": "jest --config tests/jest.config.js --testPathPattern=acceptance",
    "test:performance": "jest --config tests/jest.config.js --testPathPattern=performance",
    "test:unit": "jest --config tests/jest.config.js --testPathPattern=unit",
    "test:ci": "jest --config tests/jest.config.js --coverage --ci --watchAll=false",
    "test:debug": "jest --config tests/jest.config.js --verbose --detectOpenHandles",
    "uat": "node scripts/run-uat.js",
    "uat:workflow": "node scripts/run-uat.js --workflow",
    "uat:performance": "node scripts/run-uat.js --workflow=UAT-5",
    "uat:verbose": "node scripts/run-uat.js --verbose",
    "uat:watch": "node scripts/run-uat.js --watch",
    "uat:bail": "node scripts/run-uat.js --bail",
    "uat:coverage": "node scripts/run-uat.js --coverage",
    "uat:verify": "node verify-uat-setup.js",
    "uat:setup-test": "jest tests/uat/setup-test.ts --config tests/uat/uat.config.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "trading",
    "performance-monitoring",
    "caching",
    "database-optimization",
    "security",
    "auto-scaling",
    "alerting"
  ],
  "author": "Gamma Adaptive System Team",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "pg": "^8.11.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/gamma-adaptive-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/gamma-adaptive-system/issues"
  },
  "homepage": "https://github.com/your-org/gamma-adaptive-system#readme"
}